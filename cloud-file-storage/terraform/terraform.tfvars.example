# Copy this file to terraform.tfvars and update with your values
# terraform.tfvars is gitignored and safe for secrets

# Azure Subscription ID (REQUIRED)
# Get it with: az account show --query id -o tsv
subscription_id      = "your-subscription-id"

project_name         = "cloudfs"
environment          = "dev"
location             = "eastus"
apim_publisher_email = "your-email@example.com"  # REQUIRED - change this!

# Optional: Override defaults
# apim_publisher_name = "Your Company Name"
# tags = {
#   Owner      = "Your Team"
#   CostCenter = "Engineering"
# }


